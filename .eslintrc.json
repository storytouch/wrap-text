{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
      "no-param-reassign": "off",
      "class-methods-use-this": "off",
      "no-underscore-dangle": "off"
    }
}
